{
  "name": "aiwebstack-starter",
  "version": "1.0.0",
  "private": true,
  "description": "AI-optimized full-stack web development starter template with Next.js 15, Turborepo, Prisma, and Base UI",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "db:generate": "pnpm --filter @repo/db db:generate",
    "db:push": "pnpm --filter @repo/db db:push",
    "db:migrate": "pnpm --filter @repo/db db:migrate",
    "db:studio": "pnpm --filter @repo/db db:studio"
  },
  "devDependencies": {
    "@repo/config": "workspace:*",
    "turbo": "^2.3.3",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  }
}
